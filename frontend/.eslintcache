const hash = await uploadMetadataToIPFS(jsonData, socialLinks, userImage);\n\n    const signer = web3Provider.getSigner();\n    const contract = new Contract(signer);\n    contract.addCreator(hash).then(() => {\n      setCreatorAdded(true);\n    });\n  };\n\n  return (\n    <>\n      <DemoNavbar />\n      <main>\n        <section className=\"section landing-page section-lg\">\n          <div className=\"shape shape-style-1 shape-default\">\n            <span />\n            <span />\n            <span />\n            <span />\n            <span />\n            <span />\n            <span />\n            <span />\n            <span />\n          </div>\n          {creatorAdded && (\n            <UncontrolledAlert color=\"success\" fade={false}>\n              <span className=\"alert-inner--icon\">\n                <i className=\"ni ni-like-2\" />\n              </span>{\" \"}\n              <span className=\"alert-inner--text\">\n                <strong>Success!</strong> Creator registered transaction has\n                been sent!\n              </span>\n            </UncontrolledAlert>\n          )}\n          <Container className=\"pt-lg-7 register-overlay\">\n            <Row className=\"justify-content-center\">\n              <Col lg=\"5\">\n                <Card className=\"bg-secondary shadow border-0\">\n                  <CardHeader className=\"bg-white\">\n                    <div className=\"text-muted text-center text-lg\">\n                      Register Creator\n                    </div>\n                  </CardHeader>\n                  <CardBody className=\"px-lg-5 py-lg-5\">\n                    <Form role=\"form\" onSubmit={onSubmitHandler}>\n                      <UploadImage onImageUploaded={onImageUploadHandler} />\n                      <FormGroup>\n                        <InputGroup className=\"input-group-alternative mb-3\">\n                          <InputGroupAddon addonType=\"prepend\">\n                            <InputGroupText>\n                              <i className=\"ni ni-circle-08\" />\n                            </InputGroupText>\n                          </InputGroupAddon>\n                          <Input\n                            placeholder=\"Full Name\"\n                            name=\"full_name\"\n                            type=\"text\"\n                          />\n                        </InputGroup>\n                        <InputGroup className=\"input-group-alternative mb-3\">\n                          <InputGroupAddon addonType=\"prepend\">\n                            <InputGroupText>\n                              <i className=\"ni ni-single-02\" />\n                            </InputGroupText>\n                          </InputGroupAddon>\n                          <Input\n                            placeholder=\"Username\"\n                            name=\"username\"\n                            type=\"text\"\n                          />\n                        </InputGroup>\n                        <InputGroup className=\"input-group-alternative mb-3\">\n                          <InputGroupAddon addonType=\"prepend\">\n                            <InputGroupText>\n                              <i className=\"ni ni-single-copy-04\" />\n                            </InputGroupText>\n                          </InputGroupAddon>\n                          <Input\n                            placeholder=\"Description\"\n                            name=\"description\"\n                            type=\"textarea\"\n                          />\n                        </InputGroup>\n\n                        <InputGroup className=\"input-group-alternative mb-3\">\n                          <InputGroupAddon addonType=\"prepend\">\n                            <InputGroupText>\n                              <i className=\"fa fa-youtube\" />\n                            </InputGroupText>\n                          </InputGroupAddon>\n                          <Input\n                            placeholder=\"Youtube\"\n                            name=\"youtube\"\n                            type=\"text\"\n                          />\n                        </InputGroup>\n\n                        <InputGroup className=\"input-group-alternative mb-3\">\n                          <InputGroupAddon addonType=\"prepend\">\n                            <InputGroupText>\n                              <i className=\"fa fa-instagram\" />\n                            </InputGroupText>\n                          </InputGroupAddon>\n                          <Input\n                            placeholder=\"Instagram\"\n                            name=\"instagram\"\n                            type=\"text\"\n                          />\n                        </InputGroup>\n\n                        <InputGroup className=\"input-group-alternative mb-3\">\n                          <InputGroupAddon addonType=\"prepend\">\n                            <InputGroupText>\n                              <i className=\"fa fa-twitter\" />\n                            </InputGroupText>\n                          </InputGroupAddon>\n                          <Input\n                            placeholder=\"Twitter\"\n                            name=\"twitter\"\n                            type=\"text\"\n                          />\n                        </InputGroup>\n                      </FormGroup>\n\n                      <div className=\"text-muted font-italic\">\n                        <small>\n                          Address &nbsp;\n                          {address ? (\n                            <span className=\"text-success font-weight-700\">\n                              {address}\n                            </span>\n                          ) : (\n                            <span className=\"text-warning font-weight-700\">\n                              No Address Found\n                            </span>\